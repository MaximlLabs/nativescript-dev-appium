{
    "typescript.check.tscVersion": false,
    "window.zoomLevel": 1,
    "workbench.iconTheme": "vscode-icons",
    "vsicons.dontShowNewVersionMessage": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/lib/*.js": true,
        "**/lib/*.map": true,
        "**/lib/*.d.ts": true
    }
}