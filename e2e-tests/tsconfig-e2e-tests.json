{
    "extends": "../tsconfig",
    "include": [
        "typings/chai.d.ts",
        "typings/mocha.d.ts",
        "./*.ts",
        "../node_modules/*"
    ],
    "compilerOptions": {
        "noEmitHelpers": false,
        "importHelpers": true,
        "noEmitOnError": true,
        "baseUrl": "../",
        "paths": {
            "*": [
                "../node_modules/*"
            ]
        },
        "types": [
            "node"
        ]
    }
}