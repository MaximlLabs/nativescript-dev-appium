{
    "name": "nativescript-dev-appium",
    "version": "0.2.0",
    "description": "A NativeScript plugin to help integrate and run Appium tests",
    "main": "index.js",
    "bin": {
        "nativescript-dev-appium": "./test-runner.js"
    },
    "scripts": {
        "postinstall": "node postinstall.js",
        "appium": "nativescript-dev-appium",
        "appium-android-19": "npm run appium --runType=android19",
        "appium-android-23": "npm run appium --runType=android23",
        "appium-ios-simulator": "npm run appium --runType=ios-simulator",
        "appium-ios": "npm run appium --runType=ios"
    },
    "keywords": [
        "nativescript",
        "appium",
        "test"
    ],
    "maintainers": [
        {
            "name": "hdeshev",
            "email": "hristo.deshev@telerik.com"
        }
    ],
    "dependencies": {
        "glob": "*",
        "colors": "^1.1.2",
        "mocha": "^2.4.5",
        "portastic": "^1.0.1"
    },
    "author": "Telerik",
    "license": "MIT",
    "devDependencies": {
        "chai": "~3.5.0",
        "chai-as-promised": "~5.3.0",
        "wd": "~1.1.1"
    }
}